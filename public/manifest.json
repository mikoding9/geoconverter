  "share_target": {
    "action": "/",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "files": [
        {
          "name": "file",
          "accept": [
            ".geojson",
            ".json",
            ".zip",
            ".gpkg",
            ".kml",
            ".gpx",
            ".gml",
            ".fgb",
            ".csv",
            ".pmtiles",
            ".mbtiles"
          ]
        }
      ]
    }
  }
